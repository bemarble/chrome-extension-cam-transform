
{
  "manifest_version": 2,
  "name": "カメラ加工",
  "description": "カメラ映像を加工します",
  "version": "0.1",
  "icons":  {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/loader.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "js/main.js"
  ],
  "browser_action" : {
    "default_popup" : "html/config.html"
  }
}
